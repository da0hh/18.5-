#include <iostream>
#include <vector>
#include<algorithm>

using namespace std;

void swapvec(vector<int> vec, int *mass) {
    int temp[4];
    for (int i = 0; i < vec.size(); i++)
        mass[i] = temp[i];
    for (int i = 0; i < vec.size(); i++)
        mass[i] = vec[i];
    for (int i = 0; i < vec.size(); i++)
        temp[i] = vec[i];
}

int main() {
    setlocale(LC_ALL, "RU");
    vector<int> a = { 1,2,3,4 }; 
    int b[] = { 2,4,6,8 }; 
    
    swapvec(a, b);

    for (int i = 0; i < 4; ++i)
        std::cout << a[i];

    std::cout << std::endl;

    for (int i = 0; i < 4; ++i)
        std::cout << b[i];
}
